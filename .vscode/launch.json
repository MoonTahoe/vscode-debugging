{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "name": "Launch Program",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/index.js",
      "envFile": "${workspaceFolder}/.env",
      "runtimeArgs": ["--nolazy"]
    }
  ]
}
